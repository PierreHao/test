}
layer {
  name: "inc3a/conv3_1/scale"
  type: "Scale"
  bottom: "inc3a/conv3_1"
  top: "inc3a/conv3_1"
  param { lr_mult: 0.1  decay_mult: 0 }
  param { lr_mult: 0.1  decay_mult: 0 }
