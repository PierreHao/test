}
layer {
  name: "inc3a/relu1"
  type: "ReLU"
  bottom: "inc3a/conv1"
  top: "inc3a/conv1"
}
layer {
  name: "inc3a/conv3_1"
  type: "Convolution"
  bottom: "conv3"
  top: "inc3a/conv3_1"
  param { lr_mult: 0.1  decay_mult: 0.1 }
  param { lr_mult: 0.2  decay_mult: 0 }
  convolution_param {
    num_output: 16  kernel_size: 1
    weight_filler { type: "xavier"  std: 0.1 }
    bias_filler { type: "constant"  value: 0.1 }
  }
}
layer {
  name: "inc3a/conv3_1/bn"
  type: "BatchNorm"
  bottom: "inc3a/conv3_1"
  top: "inc3a/conv3_1"
