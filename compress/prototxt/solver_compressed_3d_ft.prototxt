net: "prototxt/trainval_compressed_3d.prototxt"
test_iter: 100
test_interval: 1000
base_lr: 0.00000000001
lr_policy: "step"
gamma: 0.9773
stepsize: 500
display: 2
max_iter: 50000
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "caffemodel/compressed_3d_ft"
solver_mode: GPU
# mini-batch size = batch_size (in trainval) * iter_size (in solver)
iter_size: 20
